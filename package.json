{
  "name": "aky100200-blog-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "test-update": "jest --updateSnapshot",
    "selenium": "mocha selenium/uiTest.js --timeout 10000",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js ",
    "lint:fix": "npm run lint --fix"
  },
  "dependencies": {
    "date-fns": "^2.16.1",
    "firebase": "^8.0.2",
    "gray-matter": "^4.0.2",
    "next": "^10.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.8.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint": "^7.13.0",
    "eslint-plugin-react": "^7.21.5",
    "jest": "^26.4.2",
    "mocha": "^8.2.1",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "./.next/",
      "./node_modules/"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.jest.json"
      }
    }
  }
}
